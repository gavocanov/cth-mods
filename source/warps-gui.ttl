@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://polyeffects.com/lv2/polywarps#bitcrusher>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-warps.html> ;
        modgui:stylesheet <modgui/stylesheet-warps.css> ;
        modgui:screenshot <modgui/screenshot-warps.png> ;
        modgui:thumbnail <modgui/thumbnail-warps.png> ;
        modgui:brand "Poly" ;
        modgui:label "Warps" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "bits" ;
            lv2:name "bits" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "xor_vs_and" ;
            lv2:name "xor vs and" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "int_osc" ;
            lv2:name "int_osc" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "input_amp" ;
            lv2:name "amp or freq" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "input_amp_2" ;
            lv2:name "input amplitude 2" ;
        ] ;
    ] .
