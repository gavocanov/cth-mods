@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://polyeffects.com/lv2/polyplaits>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-plaits.html> ;
        modgui:stylesheet <modgui/stylesheet-plaits.css> ;
        modgui:screenshot <modgui/screenshot-plaits.png> ;
        modgui:thumbnail <modgui/thumbnail-plaits.png> ;
        modgui:brand "cTh-mods" ;
        modgui:label "Plaits" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "model" ;
            lv2:name "Model" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "frequency" ;
            lv2:name "Frequency" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "harmonics" ;
            lv2:name "Harmonics" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "timbre" ;
            lv2:name "Timbre" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "morph" ;
            lv2:name "Morph" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "timbre_mod" ;
            lv2:name "Timbre mod" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "freq_mod" ;
            lv2:name "Frequency mod" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "morph_mod" ;
            lv2:name "Morph mod" ;
        ] , [
            lv2:index 8 ;
            lv2:symbol "lpg_color" ;
            lv2:name "LPG color" ;
        ] , [
            lv2:index 9 ;
            lv2:symbol "lpg_decay" ;
            lv2:name "LPG decay" ;
        ] ;
    ] .
