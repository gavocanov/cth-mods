@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://polyeffects.com/lv2/polywarps#delay>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-warps.html> ;
        modgui:stylesheet <modgui/stylesheet-warps.css> ;
        modgui:screenshot <modgui/screenshot-warps.png> ;
        modgui:thumbnail <modgui/thumbnail-warps.png> ;
        modgui:brand "Warps" ;
        modgui:label "Delay" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "speed_direction" ;
            lv2:name "speed direction" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "length" ;
            lv2:name "timbre" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "mode" ;
            lv2:name "shape" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "feedback" ;
            lv2:name "lvl-1" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "dry_wet" ;
            lv2:name "lvl-2" ;
        ] ;
    ] .
